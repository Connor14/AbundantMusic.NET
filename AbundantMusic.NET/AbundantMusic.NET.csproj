<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>A C# wrapper for the Abundant Music MIDI composer</Description>
    <Copyright>2019 Connor Schmidt</Copyright>
    <RepositoryUrl></RepositoryUrl>
    <Version>0.1.0</Version>
    <Authors>Connor14</Authors>
    <Company />
    <PackageProjectUrl>https://github.com/Connor14/AbundantMusic.NET</PackageProjectUrl>
    <PackageTags>abundant, music, midi, composer</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JavaScriptEngineSwitcher.ChakraCore" Version="3.0.9" />
    <PackageReference Include="JavaScriptEngineSwitcher.ChakraCore.Native.linux-x64" Version="3.0.9" />
    <PackageReference Include="JavaScriptEngineSwitcher.ChakraCore.Native.osx-x64" Version="3.0.9" />
    <PackageReference Include="JavaScriptEngineSwitcher.ChakraCore.Native.win-arm" Version="3.0.9" />
    <PackageReference Include="JavaScriptEngineSwitcher.ChakraCore.Native.win-x64" Version="3.0.9" />
    <PackageReference Include="JavaScriptEngineSwitcher.ChakraCore.Native.win-x86" Version="3.0.9" />
    <PackageReference Include="JavaScriptEngineSwitcher.Core" Version="3.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Update="abundant-music-composer\index.html">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\composeeditoronlinesource.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\composemain.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\composeworkersource.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\fakebytearray.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\generator.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\midi.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\midisynth.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\midisynthenvelope.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\midisynthfilter.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\midisynthinstrument.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\midisynthoscillator.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\midisynthsource.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\midisynthvoice.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\renderRequestData.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\riffwave.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="abundant-music-composer\js\stacktrace.js">
