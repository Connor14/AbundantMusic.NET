<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net45</TargetFrameworks>
    <Description>A C# wrapper for the Abundant Music MIDI composer</Description>
    <Copyright>2019 Connor Schmidt</Copyright>
    <RepositoryUrl></RepositoryUrl>
    <Version>0.2.0</Version>
    <Authors>Connor14</Authors>
    <Company />
    <PackageProjectUrl>https://github.com/Connor14/AbundantMusic.NET</PackageProjectUrl>
    <PackageTags>abundant, music, midi, composer</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReleaseNotes>Made the Generate method an async Task that returns a Composition object.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="abundant-music-composer\index.html" />
    <None Remove="abundant-music-composer\js\composeeditoronlinesource.js" />
    <None Remove="abundant-music-composer\js\composemain.js" />
    <None Remove="abundant-music-composer\js\composeworkersource.js" />
    <None Remove="abundant-music-composer\js\fakebytearray.js" />
    <None Remove="abundant-music-composer\js\generator.js" />
    <None Remove="abundant-music-composer\js\midi.js" />
    <None Remove="abundant-music-composer\js\midisynth.js" />
    <None Remove="abundant-music-composer\js\midisynthenvelope.js" />
    <None Remove="abundant-music-composer\js\midisynthfilter.js" />
    <None Remove="abundant-music-composer\js\midisynthinstrument.js" />
    <None Remove="abundant-music-composer\js\midisynthoscillator.js" />
    <None Remove="abundant-music-composer\js\midisynthsource.js" />
    <None Remove="abundant-music-composer\js\midisynthvoice.js" />
    <None Remove="abundant-music-composer\js\renderRequestData.json" />
    <None Remove="abundant-music-composer\js\riffwave.js" />
    <None Remove="abundant-music-composer\js\stacktrace.js" />
    <None Remove="abundant-music-composer\LICENSE" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="abundant-music-composer\index.html" />
    <EmbeddedResource Include="abundant-music-composer\js\composeeditoronlinesource.js" />
    <EmbeddedResource Include="abundant-music-composer\js\composemain.js" />
    <EmbeddedResource Include="abundant-music-composer\js\composeworkersource.js" />
    <EmbeddedResource Include="abundant-music-composer\js\fakebytearray.js" />
    <EmbeddedResource Include="abundant-music-composer\js\generator.js" />
    <EmbeddedResource Include="abundant-music-composer\js\midi.js" />
    <EmbeddedResource Include="abundant-music-composer\js\midisynth.js" />
    <EmbeddedResource Include="abundant-music-composer\js\midisynthenvelope.js" />
    <EmbeddedResource Include="abundant-music-composer\js\midisynthfilter.js" />
    <EmbeddedResource Include="abundant-music-composer\js\midisynthinstrument.js" />
    <EmbeddedResource Include="abundant-music-composer\js\midisynthoscillator.js" />
    <EmbeddedResource Include="abundant-music-composer\js\midisynthsource.js" />
    <EmbeddedResource Include="abundant-music-composer\js\midisynthvoice.js" />
    <EmbeddedResource Include="abundant-music-composer\js\renderRequestData.json" />
    <EmbeddedResource Include="abundant-music-composer\js\riffwave.js" />
    <EmbeddedResource Include="abundant-music-composer\js\stacktrace.js" />
    <EmbeddedResource Include="abundant-music-composer\LICENSE" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'netstandard2.0'">
    <PackageReference Include="JavaScriptEngineSwitcher.ChakraCore" Version="3.0.9" />
    <PackageReference Include="JavaScriptEngineSwitcher.ChakraCore.Native.linux-x64" Version="3.0.9" />
    <PackageReference Include="JavaScriptEngineSwitcher.ChakraCore.Native.osx-x64" Version="3.0.9" />
    <PackageReference Include="JavaScriptEngineSwitcher.ChakraCore.Native.win-arm" Version="3.0.9" />
    <PackageReference Include="JavaScriptEngineSwitcher.ChakraCore.Native.win-x64" Version="3.0.9" />
    <PackageReference Include="JavaScriptEngineSwitcher.ChakraCore.Native.win-x86" Version="3.0.9" />
    <PackageReference Include="JavaScriptEngineSwitcher.Core" Version="3.0.0" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net45'">
    <PackageReference Include="JavaScriptEngineSwitcher.ChakraCore" Version="3.0.9" />
    <PackageReference Include="JavaScriptEngineSwitcher.ChakraCore.Native.win-x64" Version="3.0.9" />
    <PackageReference Include="JavaScriptEngineSwitcher.ChakraCore.Native.win-x86" Version="3.0.9" />
    <PackageReference Include="JavaScriptEngineSwitcher.Core" Version="3.0.0" />
  </ItemGroup>
</Project>
